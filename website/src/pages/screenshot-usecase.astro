import ScreenshotLayout from "@/layouts/screenshot.astro";

import ScreenshotUsecase from "@/components/screenshot-usecase.vue";

const url = Astro.url;

const params = url.searchParams;

// Iterate over each parameter
params.forEach((value, key) => {
    console.log(`${key}: ${value}`);
});

